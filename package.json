{
  "name": "remiges",
  "version": "0.2.9",
  "main": "dist/lib/index.js",
  "repository": "git@github.com:angushtlam/remiges.git",
  "author": "angus <angushtlam@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run clean && npm run build-lib && npm run build-www",
    "build-lib": "webpack --mode production --config ./lib/webpack.config.js",
    "build-www": "webpack --mode production --config ./www/webpack.config.js",
    "clean": "rm -rf dist",
    "dev": "webpack --mode production --config ./lib/webpack.config.js --watch",
    "prepublish": "npm run build-lib",
    "storybook": "start-storybook --port 9001 --config-dir ./lib/.storybook",
    "www": "webpack-serve --config ./www/webpack.config.js --open"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "markdown-loader": "^4.0.0",
    "prettier": "1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "storybook-readme": "^4.0.0-beta1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "emotion": "^9.2.10",
    "marked": "^0.5.1",
    "react-emotion": "^9.2.10"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
