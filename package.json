{
  "name": "remiges",
  "version": "0.0.4",
  "main": "./dist/lib/index.js",
  "repository": "git@github.com:angusburg/remiges.git",
  "author": "angus <angushtlam@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run build-lib && npm run build-www",
    "build-lib": "webpack --mode production --config ./lib/webpack.config.js",
    "build-www": "webpack --mode production --config ./www/webpack.config.js",
    "dev": "start-storybook --port 9001 --config-dir ./lib/.storybook",
    "prepublish": "npm run build",
    "www": "webpack-serve --config ./www/webpack.config.js --open"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "webpack": "^4.16.5",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "styled-components": "^3.4.2",
    "webpack-serve": "^2.0.2"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
